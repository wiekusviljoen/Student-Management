@model List<Crud_Database.Models.Domain.Student>

@{
}

<h1>Students</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>School Fee</th>
            <th>Date Of Birth</th>
            <th>Class</th>
            <th> </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Email</td>
                <td>@employee.Salary</td>
                <td>@employee.DateOfBirth.ToString("dd-mm-yyyy")</td>
                <td>@employee.Department</td>
                
            </tr>
        }
    </tbody>
</table>